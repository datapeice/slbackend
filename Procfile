web: export DB_URL=$(echo $DATABASE_URL | sed 's/^postgres:\/\///') && export POSTGRES_USER=$(echo $DB_URL | cut -d: -f1) && export POSTGRES_PASSWORD=$(echo $DB_URL | sed 's/[^:]*:\(.*\)@.*/\1/') && export DB_HOST_PATH=$(echo $DB_URL | cut -d@ -f2) && export SPRING_DATASOURCE_URL="jdbc:postgresql://${DB_HOST_PATH}?sslmode=require" && java -Dspring.profiles.active=prod -Dserver.port=$PORT -jar build/libs/slbackend-0.0.1-SNAPSHOT.jar
